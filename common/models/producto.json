{
  "name": "Producto",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "comprar": {
      "type": "boolean",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "lista": {
      "type": "belongsTo",
      "model": "ListaFamiliar",
      "foreignKey": "listaFamiliarId"
    }
  },
  "acls": [],
  "methods": {
    "limpiarLista": {
      "accepts": [
        {
          "arg": "context",
          "type": "object",
          "required": true,
          "description": "Objeto de la solicitud",
          "http": {
            "source": "context"
          }
        }
      ],
      "returns": [
        {
          "arg": "listaProductos",
          "type": "array",
          "root": true,
          "description": "Productos del usuario autentificado en false"
        }
      ],
      "description": "Ponder a falso todos los productos del usuario autentificado",
      "http": [
        {
          "path": "/limpiarLista",
          "verb": "put"
        }
      ]
    }
  }
}
